citHeader("To cite corregp in publications use:")

citEntry(entry = "Misc",
	title = "corregp: Functions and Methods for Correspondence Regression.",
	author = personList(as.person("Plevoets, Koen")),
	year = "2015",
	institution = "Ghent University",

	textVersion = paste("Plevoets, Koen (2015).",
		"corregp: Functions and Methods for Correspondence Regression.",
		"Ghent University")
)
